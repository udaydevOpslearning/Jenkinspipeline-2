@Library('mylibrary') _
pipeline {
    agent any 
    tools {
        maven 'Maven'
    }

    stages {

        stage('Hello world') {
            steps {
                script {
                    hello.hello()
                }
            }
        }

        stage('checkout code from git') { 
            steps {
              
                script {
                    git_checkout.git_checkout("boxfuse-sample-java-war-hello", "master") 
                    }
                
            }
        }


    }
}

